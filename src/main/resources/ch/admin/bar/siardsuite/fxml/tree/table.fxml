    <Label fx:id="titleTableContainer">
        <styleClass>
            <String fx:value="h4" />
            <String fx:value="table-container-title" />
        </styleClass>
    </Label>
    <Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" styleClass="table-container-label" />
    <Label fx:id="nameText" alignment="CENTER" contentDisplay="CENTER" styleClass="table-container-text" />
    <Label fx:id="infoLabel" alignment="CENTER" contentDisplay="CENTER" styleClass="table-container-label" />
    <Label fx:id="infoText" alignment="CENTER" contentDisplay="CENTER" styleClass="table-container-text" />
