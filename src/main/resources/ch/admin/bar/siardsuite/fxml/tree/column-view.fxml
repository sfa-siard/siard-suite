<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="container" stylesheets="@../../css/root.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.admin.bar.siardsuite.presenter.tree.ColumnPresenter">
    <Label fx:id="titleTableContainer">
        <styleClass>
            <String fx:value="h4"/>
            <String fx:value="table-container-title"/>
        </styleClass>
    </Label>
    <HBox>
        <children>
            <VBox fx:id="labels" prefWidth="270.0">
                <Label fx:id="labelColName" />
                <Label fx:id="labelColPos" />
                <Label fx:id="labelColLob" />
                <Label fx:id="labelMime" />
                <Label fx:id="labelSql" />
                <Label fx:id="labelUdtSchema" />
                <Label fx:id="labelUdtName" />
                <Label fx:id="labelDatatyp" />
                <Label fx:id="labelUndefCol" />
                <Label fx:id="labelDefaultValue" />
                <Label fx:id="labelArrayCol" />
                <Label fx:id="labelDescCol" />

            </VBox>
            <VBox prefWidth="400.0">
                <Label fx:id="textColName" styleClass="table-container-text-small"/>
                <Label fx:id="textColPos" styleClass="table-container-text-small"/>
                <Label fx:id="textColLob" styleClass="table-container-text-small"/>
                <Label fx:id="textMime" styleClass="table-container-text-small"/>
                <Label fx:id="textSql" styleClass="table-container-text-small"/>
                <Label fx:id="textUdtSchema" styleClass="table-container-text-small"/>
                <Label fx:id="textUdtName" styleClass="table-container-text-small"/>
                <Label fx:id="textDatatyp" styleClass="table-container-text-small"/>
                <Label fx:id="textUndefCol" styleClass="table-container-text-small"/>
                <Label fx:id="textDefaultValue" styleClass="table-container-text-small"/>
                <Label fx:id="textArrayCol" styleClass="table-container-text-small"/>
                <Label fx:id="textDescCol" styleClass="table-container-text-small"/>

            </VBox>
        </children>
    </HBox>


</VBox>
