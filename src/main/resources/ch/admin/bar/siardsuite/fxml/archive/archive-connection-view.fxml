<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.TextFlow?>
<BorderPane fx:id="borderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            stylesheets="@../../css/root.css" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.admin.bar.siardsuite.presenter.archive.ArchiveConnectionPresenter">

    <center>
        <VBox BorderPane.alignment="TOP_LEFT">
            <children>
                <Text fx:id="title" styleClass="h2" strokeType="OUTSIDE" strokeWidth="0.0"/>
                <TextFlow fx:id="textFlow" lineSpacing="8.0">
                    <Text fx:id="text0" styleClass="view-text"/>
                    <Text fx:id="text1" styleClass="view-text"/>
                    <Text fx:id="text2" styleClass="view-text"/>
                    <Text fx:id="text3" styleClass="view-text"/>
                    <Text fx:id="text4" styleClass="view-text"/>
                    <VBox.margin>
                        <Insets bottom="25.0" top="25.0"/>
                    </VBox.margin>
                </TextFlow>
                <HBox prefHeight="100.0" prefWidth="600.0">
                    <children>
                        <VBox fx:id="leftVBox" prefHeight="200.0" prefWidth="300.0">
                        </VBox>
                        <VBox fx:id="rightVBox" prefHeight="200.0" prefWidth="300.0">
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets top="25.0"/>
                    </VBox.margin>
                </HBox>
                <Label fx:id="errorMessage" styleClass="error-text">
                    <VBox.margin>
                        <Insets bottom="25.0" top="25.0"/>
                    </VBox.margin>
                </Label>
            </children>
            <BorderPane.margin>
                <Insets left="50.0" right="50.0" top="50.0"/>
            </BorderPane.margin>
        </VBox>
    </center>
    <bottom>
        <HBox fx:id="buttonsBox" styleClass="btn-box" minWidth="-Infinity" maxHeight="1.7976931348623157E308"
              maxWidth="1.7976931348623157E308" spacing="20.0">
        </HBox>
    </bottom>
</BorderPane>
