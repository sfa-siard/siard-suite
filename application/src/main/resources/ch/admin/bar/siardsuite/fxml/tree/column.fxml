<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane stylesheets="@../../css/root.css" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.admin.bar.siardsuite.presenter.tree.ColumnPresenter">
    <VBox fx:id="container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox>
            <children>
                <VBox fx:id="labels" prefWidth="270.0" prefHeight="400">
                    <Label fx:id="labelColName"/>
                    <Label fx:id="labelColPos"/>
                    <Label fx:id="labelColLob"/>
                    <Label fx:id="labelMime"/>
                    <Label fx:id="labelSql"/>
                    <Label fx:id="labelUdtSchema"/>
                    <Label fx:id="labelUdtName"/>
                    <Label fx:id="labelDatatyp"/>
                    <Label fx:id="labelUndefCol"/>
                    <Label fx:id="labelDefaultValue"/>
                    <Label fx:id="labelArrayCol"/>
                    <Label fx:id="labelDescCol"/>

                </VBox>
                <VBox fx:id="texts" prefWidth="400.0" prefHeight="400"/>
            </children>
        </HBox>

        <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
        </padding>

    </VBox>
</AnchorPane>
